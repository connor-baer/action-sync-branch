  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.8.2",
    "@actions/github": "^5.0.3"
  },
  "devDependencies": {
    "@sumup/foundry": "^5.0.0",
    "@types/node": "^17.0.42",
    "@vercel/ncc": "^0.34.0",
